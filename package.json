        "ts-node": "8.6.2",
        "tslint": "6.0.0",
        "tslint-config-prettier": "1.18.0",
        "tslint-react": "4.2.0",
        "typescript": "3.8.2",
        "webpack": "4.41.6",
        "webpack-cli": "3.3.11",
